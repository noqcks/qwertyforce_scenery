    "file-type": "^16.5.4",
    "form-data": "^4.0.0",
    "json-schema-to-ts": "^2.5.5",
    "mongodb": "^4.8.1",
    "mui-file-dropzone": "^4.0.1",
    "next": "^12.2.5",
    "nodemailer": "^6.7.8",
    "nodemon": "^2.0.19",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-photo-gallery": "github:qwertyforce/react_photo_gallery_build",
